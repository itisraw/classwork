function getRandomArbitrary(min, max) {
  return Math.random() * (max - min) + min;
}
var x = Math.floor(Math.random() * 4 + 1);
if (x=1)
{
  var text = "its rainy. get a umbrella."
}
if (x=2)
{
  var text = "it heckin wimdy. put on a jacket."
}
if (x=3)
{
  var text = "PRAISE THE SUN! IT SHOWS ITSELF IN FULL MAJESTY!"
}
if (x=4)
{
  var text = "nothing special"
}
console.log (text)
